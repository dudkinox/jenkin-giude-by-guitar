pipeline {
    agent any
    stages {
        stage('Clone sources') {
            steps {
                git branch: 'main', url: 'https://github.com/dudkinox/jenkin-giude-by-guitar'
            }
        }
        stage('Check file list') {
            steps { 
                sh 'ls -l'
            }
        }
        stage('Show index.js') {
            steps { 
                sh 'cd src'
                sh 'ls -l'
                sh 'cat index.js'
            }
        }
    }
}
